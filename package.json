{
  "name": "@pilosa/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/apps/pilosa/apps/pilosa/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/pilosa/test/jest-e2e.json",
    "typeorm": "typeorm -d dist/apps/pilosa/apps/pilosa/src/config/typeorm.datasource.js",
    "migration:generate": "npm run build && npm run typeorm -- migration:generate apps/pilosa/src/db/migrations",
    "migration:run": "npm run build && npm run typeorm -- migration:run",
    "migration:show": "npm run build && npm run typeorm -- migration:show"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@aws-sdk/client-cloudwatch": "^3.592.0",
    "@aws-sdk/client-ec2": "^3.592.0",
    "@grnsft/if-plugins": "^0.3.2",
    "@grnsft/if-unofficial-plugins": "^0.3.1",
    "@influxdata/influxdb-client": "^1.33.2",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "@tgwf/co2": "^0.15.0",
    "cache-manager": "^5.6.1",
    "esbuild": "^0.21.4",
    "graphql": "^16.8.1",
    "joi": "^17.13.1",
    "jwks-rsa": "^3.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "playwright": "^1.44.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/project(|/.*)$": "<rootDir>/libs/project/src/$1",
      "^@app/cloud-aws(|/.*)$": "<rootDir>/libs/cloud-aws/src/$1",
      "^@app/cloud-core(|/.*)$": "<rootDir>/libs/cloud-core/src/$1",
      "^@app/cloud(|/.*)$": "<rootDir>/libs/cloud/src/$1",
      "^@app/web-metrics(|/.*)$": "<rootDir>/libs/web-metrics/src/$1",
      "^@app/cloud-metrics(|/.*)$": "<rootDir>/libs/cloud-metrics/src/$1",
      "^@app/health(|/.*)$": "<rootDir>/libs/health/src/$1",
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@app/user(|/.*)$": "<rootDir>/libs/user/src/$1",
      "^@app/metrics(|/.*)$": "<rootDir>/libs/metrics/src/$1",
      "^@app/synthetic-scan(|/.*)$": "<rootDir>/libs/synthetic-scan/src/$1"
    }
  }
}
