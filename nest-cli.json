{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/pilosa/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/pilosa/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/pilosa",
  "projects": {
    "pilosa": {
      "type": "application",
      "root": "apps/pilosa",
      "entryFile": "main",
      "sourceRoot": "apps/pilosa/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pilosa/tsconfig.app.json"
      }
    },
    "project": {
      "type": "library",
      "root": "libs/project",
      "entryFile": "index",
      "sourceRoot": "libs/project/src",
      "compilerOptions": {
        "tsConfigPath": "libs/project/tsconfig.lib.json"
      }
    },
    "cloud-aws": {
      "type": "library",
      "root": "libs/cloud-aws",
      "entryFile": "index",
      "sourceRoot": "libs/cloud-aws/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cloud-aws/tsconfig.lib.json"
      }
    },
    "cloud": {
      "type": "library",
      "root": "libs/cloud",
      "entryFile": "index",
      "sourceRoot": "libs/cloud/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cloud/tsconfig.lib.json"
      }
    },
    "web-metrics": {
      "type": "library",
      "root": "libs/web-metrics",
      "entryFile": "index",
      "sourceRoot": "libs/web-metrics/src",
      "compilerOptions": {
        "tsConfigPath": "libs/web-metrics/tsconfig.lib.json"
      }
    },
    "cloud-metrics": {
      "type": "library",
      "root": "libs/cloud-metrics",
      "entryFile": "index",
      "sourceRoot": "libs/cloud-metrics/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cloud-metrics/tsconfig.lib.json"
      }
    },
    "health": {
      "type": "library",
      "root": "libs/health",
      "entryFile": "index",
      "sourceRoot": "libs/health/src",
      "compilerOptions": {
        "tsConfigPath": "libs/health/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "metrics": {
      "type": "library",
      "root": "libs/metrics",
      "entryFile": "index",
      "sourceRoot": "libs/metrics/src",
      "compilerOptions": {
        "tsConfigPath": "libs/metrics/tsconfig.lib.json"
      }
    },
    "synthetic-scan": {
      "type": "library",
      "root": "libs/synthetic-scan",
      "entryFile": "index",
      "sourceRoot": "libs/synthetic-scan/src",
      "compilerOptions": {
        "tsConfigPath": "libs/synthetic-scan/tsconfig.lib.json"
      }
    }
  }
}